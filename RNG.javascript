javascript:(function(){    let bestAura = null, lastMove = 0, auraHistory = [];    const auraData = [        { name: "Common", chance: "1 in 2", color: "gray", min: 1, max: 80 },        { name: "Uncommon", chance: "1 in 4", color: "green", min: 81, max: 140 },        { name: "Rare", chance: "1 in 8", color: "blue", min: 141, max: 180 },        { name: "Epic", chance: "1 in 32", color: "purple", min: 181, max: 230 },        { name: "Legendary", chance: "1 in 128", color: "gold", min: 231, max: 245 },        { name: "Divine", chance: "1 in 256", color: "white", min: 246, max: 252 },        { name: "Celestial", chance: "1 in 512", color: "linear-gradient(to right, cyan, blue, white)", min: 253, max: 254 },        { name: "Abyssal", chance: "1 in 1024", color: "linear-gradient(to right, black, purple, red)", min: 255, max: 255 },        { name: "Omni", chance: "1 in 2048", color: "linear-gradient(to right, red, gold, cyan, purple)", min: 256, max: 256 }    ];    function showAuraTracker() {        let panel = document.getElementById("aura-tracker");        if (panel) { panel.style.display = panel.style.display === "none" ? "block" : "none"; return; }        panel = document.createElement("div");        panel.id = "aura-tracker";        Object.assign(panel.style, {            position: "fixed", top: "10px", right: "10px", padding: "10px",            background: "rgba(0,0,0,0.8)", color: "white", border: "1px solid white",            fontSize: "14px", maxHeight: "200px", overflowY: "auto", zIndex: "9999"        });        let closeBtn = document.createElement("button");        closeBtn.textContent = "Close";        closeBtn.onclick = () => { panel.style.display = "none"; };        let content = document.createElement("div");        content.id = "aura-content";        panel.append(closeBtn, content);        document.body.appendChild(panel);    }    function generateAura() {        let rngValue = Math.floor(Math.random() * 256) + 1;        return auraData.find(aura => rngValue >= aura.min && rngValue <= aura.max);    }    document.addEventListener("mousemove", function(e) {        let now = Date.now();        if (now - lastMove < 500) return;        lastMove = now;        let aura = generateAura();        let span = document.createElement("span");        span.textContent = `${aura.name}: ${aura.chance}`;        Object.assign(span.style, {            position: "fixed", left: `${e.clientX}px`, top: `${e.clientY}px`,            fontSize: "16px", color: aura.color, pointerEvents: "none",            background: "transparent", webkitBackgroundClip: "text", fontWeight: "bold"        });        document.body.appendChild(span);        setTimeout(() => span.remove(), 1000);        if (["Divine", "Celestial", "Abyssal", "Omni"].includes(aura.name)) {            auraHistory.push(aura);            bestAura = aura;            updateBestAuraDisplay();            if (aura.name === "Omni") triggerCutscene("ðŸ”¥ **OMNI AURA UNLOCKED** ðŸ”¥");            if (aura.name === "Abyssal") triggerCutscene("ðŸŒ‘ **ABYSSAL POWER AWAKENS** ðŸŒ‘");        }    });    function updateBestAuraDisplay() {        let auraElement = document.getElementById("best-aura");        if (!auraElement) {            auraElement = document.createElement("div");            auraElement.id = "best-aura";            Object.assign(auraElement.style, {                position: "fixed", pointerEvents: "none",                fontSize: "18px", fontWeight: "bold"            });            document.body.appendChild(auraElement);        }        if (bestAura) {            auraElement.textContent = bestAura.name;            auraElement.style.color = bestAura.color;            auraElement.style.left = `${window.innerWidth / 2}px`;            auraElement.style.top = `10px`;        }    }    function triggerCutscene(message) {        let cutscene = document.createElement("div");        Object.assign(cutscene.style, {            position: "fixed", top: "0", left: "0", width: "100%", height: "100%",            background: "rgba(0, 0, 0, 0.9)", color: "white", display: "flex",            alignItems: "center", justifyContent: "center", fontSize: "40px",            zIndex: "9999", transition: "opacity 2s"        });        cutscene.innerHTML = message;        document.body.appendChild(cutscene);        setTimeout(() => { cutscene.style.opacity = "0"; setTimeout(() => cutscene.remove(), 2000); }, 3000);    }    let trackerBtn = document.createElement("button");    trackerBtn.textContent = "Show Auras";    Object.assign(trackerBtn.style, {        position: "fixed", bottom: "10px", right: "10px", padding: "5px 10px",        background: "black", color: "white", border: "1px solid white", cursor: "pointer"    });    trackerBtn.onclick = function() {        showAuraTracker();        let content = document.getElementById("aura-content");        let sortedAuras = auraHistory.sort((a, b) => auraData.findIndex(aura => aura.name === b.name) - auraData.findIndex(aura => aura.name === a.name));        content.innerHTML = sortedAuras.map(aura => `<div style="color:${aura.color}">${aura.name}: ${aura.chance}</div>`).join("");    };    document.body.appendChild(trackerBtn);})();
